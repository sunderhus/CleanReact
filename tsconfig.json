{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "esModuleInterop": true,
    "strict": false,
    "noFallthroughCasesInSwitch": true,
    "allowUnreachableCode": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@/*":["*"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": ["src/main/test/cypress"]
}
